{
  "name": "open-weather-map-rest-api",
  "version": "1.0.0",
  "description": "RESTful API performing weather api requests",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-esm migration:generate src/migrations/Migration -d ./src/data-source.ts",
    "migration:create": "npm run typeorm migration:create",
    "migration:run": "npm run typeorm migration:run -- -d src/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/data-source.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Mahdi Taleghani",
  "license": "MIT",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "country-state-city": "^3.2.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "pg": "^8.4.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^16.11.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.34.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^2.8.8",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "4.5.2"
  }
}
